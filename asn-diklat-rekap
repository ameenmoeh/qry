select * from TM_MUTASI
select top 100 * from TD_TAKAH where ALMARI like 'L1%'

SELECT b.KODE_LEVEL_JABATAN, b.LEVEL_JABATAN, count(a.nip) jumlah from vwDATA_PEGAWAI a
left JOIN TM_LEVEL_JABATAN b on b.KODE_LEVEL_JABATAN=a.KODE_LEVEL_JABATAN
where a.KODE_MUTASI=1 
group by b.KODE_LEVEL_JABATAN, b.LEVEL_JABATAN
order by b.LEVEL_JABATAN

select * from TM_GRUP_JABATAN

select * from vwDATA_PEGAWAI WHERE KODE_LEVEL_JABATAN='227'

select A.KODE_SATUAN_KERJA, A.KODE_ATASAN, A.SATUAN_KERJA, C.LEVEL_JABATAN, A.KODE_UNIT_KERJA, A.PIMPINAN, A.ESELON, count(B.NIP) AS ABK
from TM_SATUAN_KERJA A LEFT JOIN 
    vwDATA_PEGAWAI B ON A.KODE_SATUAN_KERJA = B.KODE_SATUAN_KERJA LEFT JOIN
    TM_LEVEL_JABATAN C ON B.KODE_LEVEL_JABATAN = C.KODE_LEVEL_JABATAN
WHERE b.KODE_MUTASI=1
GROUP by A.KODE_SATUAN_KERJA, A.KODE_ATASAN, A.SATUAN_KERJA, C.LEVEL_JABATAN, A.KODE_UNIT_KERJA, A.PIMPINAN, A.ESELON
order by A.KODE_SATUAN_KERJA


SELECT * from TM_REF
SELECT * FROM TM_LEVEL_JABATAN

SELECT TOP 100 * FROM TM_PEGAWAI WHERE KODE_MUTASI='1' AND USER_UPDATE='ameen'

--UPDATE TM_PEGAWAI SET KODE_MUTASI=3, USER_UPDATE='ameen', TIME_UPDATE=GETDATE() WHERE NIP_BARU='196212281994032002'

SELECT * from TS_USER

select A.KODE_SATUAN_KERJA, A.KODE_ATASAN, A.SATUAN_KERJA, A.KODE_UNIT_KERJA, A.PIMPINAN, A.ESELON, count(B.NIP) AS JML_PEGAWAI
from TM_SATUAN_KERJA A LEFT JOIN 
    vwDATA_PEGAWAI B ON A.KODE_SATUAN_KERJA = B.KODE_SATUAN_KERJA 
WHERE b.KODE_MUTASI=1
GROUP by A.KODE_SATUAN_KERJA, A.KODE_ATASAN, A.SATUAN_KERJA, A.KODE_UNIT_KERJA, A.PIMPINAN, A.ESELON
order by A.KODE_SATUAN_KERJA

SELECT b.KODE_TIPE_DIKLAT, b.diklat, c.tipe_diklat, a.kode_diklat, a.tahun from TR_DIKLAT a 
    left join tm_diklat b on a.kode_diklat=b.kode_diklat 
    LEFT join tm_tipe_diklat c on b.KODE_TIPE_DIKLAT=c.KODE_TIPE_DIKLAT
    WHERE a.tahun in ('2019','2020','2021','2022','2023') 

SELECT top 100 * from tr_diklat
SELECT * from tm_diklat
SELECT * from TM_REF
SELECT * FROM tm_ref_group
SELECT * FROM tm_tipe_diklat
SELECT top 100 * from vwDATA_PEGAWAI
SELECT TOP 100 * FROM TD_LAIN
SELECT TOP 100 * FROM TD_ALAMAT

